{
  "name": "sls-openapi-helpers",
  "version": "0.0.0-development",
  "description": "Tooling and helper commands to make it easier to work with Serverless and OpenAPI",
  "bin": {
    "sls-openapi": "./bin/run"
  },
  "scripts": {
    "clean": "rm -rf ./lib/",
    "build": "npm run clean && tsc -b",
    "cm": "cz",
    "coverage": "codecov",
    "lint": "eslint ./src --ext .ts",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "type-check": "tsc --noEmit",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'"
  },
  "main": "./lib/index.js",
  "files": [
    "lib/**/*",
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distinction-dev/package-name.git"
  },
  "license": "MIT",
  "author": {
    "name": "Himanshu Pant",
    "email": "himanshupant127@gmail.com",
    "url": "https://github.com/killdozerx2"
  },
  "keywords": [
    "firebase",
    "frontend",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/distinction-dev/package-name/issues"
  },
  "homepage": "https://github.com/distinction-dev/package-name#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "dependencies": {
    "@oclif/core": "^1.20.4",
    "@oclif/plugin-help": "^5.1.19",
    "@oclif/plugin-plugins": "^2.1.7",
    "openapi-types": "^12.0.2"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@oclif/test": "^2.2.13",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^10.0.0",
    "@types/node": "18.11.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "chai": "^4.3.6",
    "codecov": "^3.8.3",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.25.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "mocha": "^10.1.0",
    "oclif": "^3.4.1",
    "prettier": "^2.2.1",
    "semantic-release": "^19.0.2",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.0-dev.20220916"
  },
  "oclif": {
    "bin": "sls-openapi",
    "dirname": "sls-openapi",
    "commands": "./lib/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
